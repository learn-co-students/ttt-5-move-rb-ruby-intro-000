#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here

puts "Welcome to Tic Tac Toe!"

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]

puts "Where would you like to go?"

string = gets.strip

input_to_index(string)

index = string.to_i - 1

move(board, index, marker = "X")

display_board(board)
