#!/usr/bin/env ruby
puts "Welcome to Tic Tac Toe!"
require_relative '../lib/move.rb'

puts "Where would you like to go?"
input = gets.strip
board = [" "," "," "," "," "," "," "," "," "]
index = input_to_index(input)
the_board_array,the_index,the_players_character="X"
move(board, index)

display_board (board)
