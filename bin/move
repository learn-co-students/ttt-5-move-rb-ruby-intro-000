#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here

puts "Welcome to Tic Tac Toe!\n "
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
display_board(board)
puts "\nWhere would you like to go?"
input = gets.strip
index = input_to_index(input)
move(board, index)
display_board(board)
