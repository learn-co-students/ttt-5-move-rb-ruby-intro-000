#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]

puts "Where would you like to go?"
input_index = gets.strip
index = input_to_index(input_index)

move(board, index)

display_board(board)
