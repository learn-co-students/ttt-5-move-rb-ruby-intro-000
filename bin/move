#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "];
puts  "Welcome to Tic Tac Toe!"
puts  "Where would you like to go?"
your_move = gets.strip
index = input_to_index(your_move)
move(board, index, "X")
display_board(board)
