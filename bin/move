#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here

puts "Welcome to Tic Tac Toe!"
puts "Where would you like to go?"
input = gets.chomp
# input_to_index(input)
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
move(board, input_to_index(input), char = "X")
display_board(board)
