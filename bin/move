
#!/usr/bin/env ruby

require_relative  '../lib/move.rb'
board = [" "," "," "," "," "," "," "," "," "]
puts "Welcome to Tic Tac Toe!"



puts"where would you like to go?"
puts "please choose the 
number between 1-9"

i = input_to_index(gets.strip)
puts "x or an o"


