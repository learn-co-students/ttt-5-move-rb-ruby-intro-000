#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
board = [' ',' ',' ',' ',' ',' ',' ',' ',' ']
 puts 'Welcome to Tic Tac Toe!'

puts display_board(board)
puts 'Where would you like to go?'
input = gets.strip
idx = input_to_index(input)
move(board,idx)
display_board(board)
