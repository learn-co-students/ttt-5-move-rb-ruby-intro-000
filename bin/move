#!/usr/bin/env ruby

require_relative '../lib/move.rb'
board = [ " ", " ", " ", " ", " ", " ", " ", " ", " " ]
# Code your CLI Here
puts "Welcome to Tic Tac Toe!"
puts "Where would you like to go?"
position = gets.strip
index = input_to_index(position)
move(board, index, token = "X")

puts display_board(board)
