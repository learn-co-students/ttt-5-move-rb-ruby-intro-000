#!/usr/bin/env ruby

require_relative '../lib/move.rb'




# Code your CLI Here


board = [" ", " ", " " , " ", " " , " ", " " , " ", " "]

puts "Welcome to Tic Tac Toe!"
puts  "Where would you like to go?"
answer = gets.strip
answer = input_to_index(answer)

move(board, answer)

display_board(board)
